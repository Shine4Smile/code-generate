{
  "name": "acm-template-generate-pro",
  "description": "ACM求和示例模板",
  "basePackage": "com.milky",
  "version": "1.0",
  "author": "周树人",
  "createTime": "2023-12-31",
  "fileConfig": {
    "inputRootPath": ".source/acm-template",
    "outputRootPath": "generated",
    "sourceRootPath": "D:/Project/GitProject/code-generate/code-generate-samples/acm-template",
    "type": "dir",
    "files": [
      {
        "inputPath": "/src/com/simple/acm/NumberSumTemplate.java.ftl",
        "outputPath": "/src/com/simple/acm/NumberSumTemplate.java",
        "type": "file",
        "generateType": "dynamic"
      },
      {
        "inputPath": "_gitignore",
        "outputPath": "_gitignore",
        "type": "file",
        "generateType": "static"
      },
      {
        "inputPath": "README.md",
        "outputPath": "README.md",
        "type": "file",
        "generateType": "static"
      }
    ]
  },
  "modelConfig": {
    "models": [
      {
        "fieldName": "loop",
        "type": "boolean",
        "description": "是否生成循环",
        "defaultValue": false,
        "abbr": "l"
      },
      {
        "fieldName": "author",
        "type": "String",
        "description": "作者注释",
        "defaultValue": "simple",
        "abbr": "a"
      },
      {
        "fieldName": "outputText",
        "type": "String",
        "description": "输出说明",
        "defaultValue": "输出结果",
        "abbr": "o"
      }
    ]
  }
}